{
  "name": "milwad/laravel-validate",
  "description": "Write rules easy & fast",
  "keywords": [
    "milwad",
    "laravel-validate",
    "validate",
    "regex"
  ],
  "homepage": "https://github.com/milwad-dev/laravel-validate",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
      {
          "name": "milwad khosravi",
          "email": "milwad.dev@gmail.com",
          "homepage": "https://milwad.ir",
          "role": "Developer"
      }
  ],
  "require": {
     "php": "^7.2|^8.1",
      "laravel/framework": "5.*|6.*|7.*|8.*|9.*"
  },
  "autoload": {
    "psr-4": {
        "Milwad\\LaravelValidate\\": "src/"
    }
  },
  "config": {
     "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "extra": {
     "laravel": {
         "providers": [
             "Milwad\\LaravelValidate\\LaravelValidateServiceProvider"
         ]
     }
  }
}
